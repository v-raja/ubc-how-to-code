/**
 * 16-way Or:
 * out = (in[0] or in[1] or ... or in[15])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    Or8Way (in=in[0..7], out=first);
    Or8Way (in=in[8..15], out=second);
    Or (a=first, b=second, out=out);
}
