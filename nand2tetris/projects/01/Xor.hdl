/**
 * Exclusive-or gate:
 * out = not (a == b)
 * (not (a and b)) and (a or b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And (a=a, b=b, out=aAndb);
    Not (in=aAndb, out=NotaAndb);
    Or  (a=a, b=b, out=aOrb);
    And (a=aOrb, b=NotaAndb, out=out);
}
